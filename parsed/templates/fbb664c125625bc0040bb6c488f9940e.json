{"components/blog/HomePagePost":"\r\n  <div class=\"columns large-4 medium-4 mob-large-4\">\r\n    <div class=\"home-blog-item\">\r\n      {{#if thumbnail}}\r\n        <div class=\"home-blog-img banner-hover\">\r\n            {{#if thumbnail}}\r\n              <a href=\"{{url}}\">\r\n                <span>\r\n                  <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage thumbnail 'gallery'}}\" alt=\"{{thumbnail.alt}}\">\r\n                </span>\r\n              </a>\r\n              {{else}}\r\n              <a href=\"{{url}}\">\r\n                <img src=\"{{cdn ../../theme_settings.default_image_blog}}\" alt=\"No Image\" title=\"No Image\">\r\n              </a>\r\n            {{/if}}\r\n        </div>\r\n        {{/if}}\r\n        <div class=\"home-blog-detail\">\r\n          <h2>\r\n            <a href=\"{{url}}\" class=\"home-blog-title\">{{title}}</a>\r\n          </h2>\r\n            <p>\r\n                {{#if body}}\r\n                    {{{body}}}\r\n                {{else}}\r\n                    {{summary}}\r\n                {{/if}}\r\n            </p>\r\n            {{#if show_read_more}}\r\n              <a class=\"read-more\" href=\"{{url}}\">read more</a>\r\n            {{/if}}\r\n          </div>\r\n    </div>\r\n  </div>\r\n"}