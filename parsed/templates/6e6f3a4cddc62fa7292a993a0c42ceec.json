{"components/common/navigation-menu":"<nav class=\"navPages\">\r\n  <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}} sf-menu\" data-speed=\"0.8\">\r\n    <li class=\"navPages-item navPages-home {{theme_settings.mega_menu}}\">\r\n      <a class=\"navPages-action\" href=\"{{urls.home}}\" class=\"needsclick\">Home</a>\r\n    </li>\r\n\r\n    {{#if theme_settings.headernavigation '==' \"navcategory\"}}\r\n        <li class=\"{{#if @last}}last-menu-item{{/if}}\">\r\n            {{#each categories}}\r\n                  <li class=\"navPages-item\">\r\n                    {{> components/common/navigation-list}}\r\n                  </li>\r\n            {{/each}}\r\n        </li>\r\n    {{/if}}\r\n\r\n\r\n    {{#if theme_settings.headernavigation '==' \"navwebpage\"}}\r\n      {{#each pages}}\r\n           <li class=\"navPages-item navPages-item-page {{#if @last}}last-menu-item{{/if}}\">\r\n             {{> components/common/navigation-list}}\r\n           </li>\r\n       {{/each}}\r\n    {{/if}}\r\n\r\n    {{#if theme_settings.headernavigation '==' \"navboth\"}}\r\n      <li class=\"\">\r\n        <a class=\"needsclick\" href=\"/categories\">shop all <span class=\"icon-arrow-1-down\"></span></a>\r\n        <ul class=\"\">\r\n          {{#each categories}}\r\n            <li class=\"navPages-item {{#and @last (if (length pages) '==' '0')}}last-menu-item{{/and}} nav-arrow-roted\">\r\n              {{> components/common/navigation-list}}\r\n            </li>\r\n          {{/each}}\r\n        </ul>\r\n      </li>\r\n      {{#each pages}}\r\n           <li class=\"navPages-item navPages-item-page {{#if @last}}last-menu-item{{/if}}\">\r\n             {{> components/common/navigation-list}}\r\n           </li>\r\n       {{/each}}\r\n    {{/if}}\r\n  </ul>\r\n</nav>\r\n","components/common/navigation-list":"<!-- SECOND LEVEL STARTS -->\r\n{{#if children}}\r\n<a href=\"{{url}}\" class=\"needsclick\">\r\n    {{name}}\r\n    <span class=\"icon-arrow-1-down\"></span>\r\n</a>\r\n<ul class=\"navlist-sub nav-second-level\">\r\n    {{#each children}}\r\n    <li>\r\n        <!-- THIRD LEVEL STARTS -->\r\n        {{#if children}}\r\n        <a href=\"{{url}}\" class=\"needsclick\">\r\n            {{name}} <span class=\"icon-arrow-1-right\"></span>\r\n        </a>\r\n        <ul class=\"navlist-sub nav-third-level\">\r\n            {{#each children}}\r\n            <li>\r\n\r\n              <!-- FOURTH LEVEL STARTS -->\r\n              {{#if children}}\r\n              <a href=\"{{url}}\" class=\"needsclick\">\r\n                  {{name}} <span class=\"icon-arrow-1-right\"></span>\r\n              </a>\r\n              <ul class=\"navlist-sub nav-third-level\">\r\n                  {{#each children}}\r\n                  <li>\r\n                      <a href=\"{{url}}\" class=\"needsclick\">{{name}} </a>\r\n                  </li>\r\n                  {{/each}}\r\n              </ul>\r\n              {{else}}\r\n                <a href=\"{{url}}\" class=\"needsclick\">{{name}}</a>\r\n              {{/if}}\r\n              <!-- FOURTH LEVEL STARTS -->\r\n\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n        {{else}}\r\n        <a href=\"{{url}}\" class=\"needsclick\">{{name}}</a>\r\n        {{/if}}\r\n        <!-- THIRD LEVEL STARTS -->\r\n    </li>\r\n    {{/each}}\r\n</ul>\r\n{{else}}\r\n<a href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n<!-- SECOND LEVEL ENDS -->\r\n"}