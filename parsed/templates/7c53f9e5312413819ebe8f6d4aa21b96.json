{"components/common/navigation":"<nav class=\"navUser\">\r\n    <ul class=\"navUser-section navUser-section--alt account-section sf-menu\">\r\n      {{> components/common/header-account-menu}}\r\n\r\n      {{#if theme_settings.EnableProductCompare}}\r\n      <li class=\"navUser-item navUser-item-compare\">\r\n          <a class=\"navUser-action navUser-item--compare needsclick\" href=\"{{urls.compare}}\" data-compare-nav>\r\n            <i class=\"icon-shuffle\"></i>\r\n            <span class=\"countPill countPill--positive countPill--alt\"></span>\r\n          </a>\r\n      </li>\r\n      {{/if}}\r\n\r\n\r\n      <li class=\"navUser-item navUser-item--cart\">\r\n          <a  class=\"navUser-action\"\r\n              data-cart-preview\r\n              data-dropdown=\"cart-preview-dropdown\"\r\n              data-options=\"align:right\"\r\n              href=\"{{urls.cart}}\">\r\n              <span class=\"navUser-item-cartLabel\"><i class=\"icon-shopping-bag\"></i></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n          </a>\r\n          <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n      </li>\r\n      <li class=\"navUser-item\">\r\n          <a class=\"navUser-action navUser-action--quickSearch\" href=\"javascript:void(0);\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\"><i class=\"icon-magnifier-1\"></i></a>\r\n          <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n              {{> components/common/quick-search headerSearch='headerSearch'}}\r\n          </div>\r\n      </li>\r\n      <li class=\"navUser-item desktop_mobile_menu\">\r\n        <a class=\"control-otherlinks\" href=\"javascript:void(0);\" onclick=\"\">\r\n          <span class=\"otherlinks-line-1\"></span>\r\n          <span class=\"otherlinks-line-2\"></span>\r\n          <span class=\"otherlinks-line-3\"></span>\r\n        </a>\r\n      </li>\r\n    </ul>\r\n\r\n</nav>\r\n\r\n<div id=\"headerSidebar\" class=\"header-side-bar\">\r\n\r\n  <div class=\"close\">\r\n    <a class=\"close-icon\" href=\"javascript:void(0);\"><i class=\"icon-close\"></i></a>\r\n  </div>\r\n\r\n  <div class=\"category\">\r\n    <h5 class=\"sidebarBlock-heading\">{{lang 'footer.categories'}}</h5>\r\n    <ul class=\"navList navList-treeview\">\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n              {{> components/common/navigation-list}}\r\n            </li>\r\n      {{/each}}\r\n    </ul>\r\n  </div>\r\n\r\n  <div class=\"webpages m-t-30\">\r\n    <h5 class=\"sidebarBlock-heading\">{{lang 'footer.navigate'}}</h5>\r\n    <ul class=\"navList navList-treeview\">\r\n      {{#each pages}}\r\n           <li class=\"navPages-item navPages-item-page\">\r\n             {{> components/common/navigation-list}}\r\n           </li>\r\n       {{/each}}\r\n    </ul>\r\n  </div>\r\n\r\n  <div class=\"brand m-t-30\">\r\n    <h5 class=\"sidebarBlock-heading\">{{lang 'footer.brands'}}</h5>\r\n    <ul class=\"navList navList-treeview\">\r\n      {{#each shop_by_brand}}\r\n          {{#if @index '<' '5' }}\r\n            <li>\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n          {{/if}}\r\n      {{/each}}\r\n    </ul>\r\n  </div>\r\n\r\n</div>\r\n","components/common/quick-search":"<div class=\"close\">\r\n  <a class=\"close-icon\" href=\"javascript:void(0);\"><i class=\"icon-close\"></i></a>\r\n</div>\r\n{{#if headerSearch}}\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"\" action=\"{{urls.search}}\" id=\"headerQuickSearchForm\" class=\"searchform\">\r\n        <fieldset class=\"\">\r\n            <div class=\"\">\r\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                <input type=\"submit\" data-empty-message=\"Please write any keyword to Search.\"  class=\"submitBtn navUser-action--quickSearch u-hiddenVisually\" value=\"Search\">\r\n                <span class=\"icon-magnifier-1\" onclick=\"document.getElementById('headerQuickSearchForm').submit();\"></span>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n{{/if}}\r\n","components/common/header-account-menu":"<li class=\"navUser-item navUser-item--account\">\r\n    <a class=\"navUser-action needsclick\" href=\"{{urls.account.index}}\">\r\n      <i class=\"icon-user\"></i>\r\n      <!-- <span>{{lang 'common.account'}}</span> -->\r\n    </a>\r\n    <ul class=\"navUser-section-sub section-sub-right\">\r\n      {{#if customer}}\r\n        <li class=\"navUser-item\">\r\n            <a class=\"navUser-action needsclick\" href=\"{{urls.auth.logout}}\">\r\n              <span>{{lang 'common.logout'}}</span>\r\n            </a>\r\n        </li>\r\n      {{else}}\r\n        <li class=\"navUser-item\">\r\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.login}}\">\r\n            <span>{{lang 'common.login'}}</span>\r\n          </a>\r\n        </li>\r\n        {{#if settings.account_creation_enabled}}\r\n        <li class=\"navUser-item\">\r\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.create_account}}\">\r\n            </i><span>{{lang 'common.sign_up'}}</span>\r\n          </a>\r\n        </li>\r\n        {{/if}}\r\n      {{/if}}\r\n      {{#if settings.gift_certificates_enabled}}\r\n          <li class=\"navUser-item\">\r\n              <a class=\"navUser-action needsclick\" href=\"{{urls.gift_certificate.purchase}}\">\r\n              <span>{{lang 'common.gift_cert'}}</span>\r\n              </a>\r\n          </li>\r\n      {{/if}}\r\n      {{#if settings.show_wishlist}}\r\n        <li class=\"navUser-item\">\r\n            <a class=\"navUser-action\" href=\"{{urls.account.wishlists.all}}\">\r\n              <span>{{#if customer.num_wishlists}}{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}{{else}}{{lang 'account.nav.wishlists' num_wishlists='0'}}{{/if}}</span>\r\n            </a>\r\n        </li>\r\n      {{/if}}\r\n\r\n      {{#if customer.store_credit.value '>' 0}}\r\n          <li class=\"navUser-item navUser-item-storeCredit\">\r\n              <a class=\"navUser-action navUser-action--storeCredit needsclick\" href=\"{{urls.cart}}\">\r\n                 <span>{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\r\n              </a>            \r\n          </li>\r\n      {{/if}}\r\n    {{#if currency_selector.currencies.length '>' 1}}\r\n      <li class=\"navUser-item\">\r\n        <div class=\"currency\">\r\n          {{> components/common/currency-selector}}\r\n        </div>\r\n      </li>\r\n    {{/if}}\r\n    </ul>\r\n</li>\r\n","components/common/navigation-list":"<!-- SECOND LEVEL STARTS -->\r\n{{#if children}}\r\n<a href=\"{{url}}\" class=\"needsclick\">\r\n    {{name}}\r\n    <span class=\"icon-arrow-1-down\"></span>\r\n</a>\r\n<ul class=\"navlist-sub nav-second-level\">\r\n    {{#each children}}\r\n    <li>\r\n        <!-- THIRD LEVEL STARTS -->\r\n        {{#if children}}\r\n        <a href=\"{{url}}\" class=\"needsclick\">\r\n            {{name}} <span class=\"icon-arrow-1-right\"></span>\r\n        </a>\r\n        <ul class=\"navlist-sub nav-third-level\">\r\n            {{#each children}}\r\n            <li>\r\n\r\n              <!-- FOURTH LEVEL STARTS -->\r\n              {{#if children}}\r\n              <a href=\"{{url}}\" class=\"needsclick\">\r\n                  {{name}} <span class=\"icon-arrow-1-right\"></span>\r\n              </a>\r\n              <ul class=\"navlist-sub nav-third-level\">\r\n                  {{#each children}}\r\n                  <li>\r\n                      <a href=\"{{url}}\" class=\"needsclick\">{{name}} </a>\r\n                  </li>\r\n                  {{/each}}\r\n              </ul>\r\n              {{else}}\r\n                <a href=\"{{url}}\" class=\"needsclick\">{{name}}</a>\r\n              {{/if}}\r\n              <!-- FOURTH LEVEL STARTS -->\r\n\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n        {{else}}\r\n        <a href=\"{{url}}\" class=\"needsclick\">{{name}}</a>\r\n        {{/if}}\r\n        <!-- THIRD LEVEL STARTS -->\r\n    </li>\r\n    {{/each}}\r\n</ul>\r\n{{else}}\r\n<a href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n<!-- SECOND LEVEL ENDS -->\r\n","components/common/currency-selector":"<ul class=\"navUser-section\">\r\n    {{#if currency_selector.currencies.length '>' 1}}\r\n\r\n      <li class=\"navUser-item\">\r\n          <a class=\"navUser-action has-dropdown\" href=\"javascript:void(0)\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">\r\n            <span>{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\r\n          </a>\r\n      </li>\r\n    {{/if}}\r\n</ul>\r\n\r\n<ul class=\"currencyOption clearfix\">\r\n{{#each currency_selector.currencies}}\r\n<li>\r\n      {{#if is_active}}\r\n        <a class=\"active\" title=\"{{name}}\" href=\"{{{switch_url}}}\">\r\n          {{{flag}}}\r\n        </a>\r\n      {{else}}\r\n        <a href=\"{{{switch_url}}}\" title=\"{{name}}\">\r\n          {{{flag}}}\r\n        </a>\r\n      {{/if}}\r\n</li>\r\n{{/each}}\r\n</ul>\r\n"}