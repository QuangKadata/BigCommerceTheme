{"components/common/instagram1":"\r\n{{#and theme_settings.instagramId theme_settings.instagramKey}}\r\n    <article class=\"instagrame m-b-50 clearfix\" data-section-type=\"storeInfo\">\r\n      <div class=\"section-title\">\r\n        {{#if theme_settings.instagramtitle}}\r\n        <h2 class=\"page-heading\"><span class=\"insta-black-logo\"><svg class=\"footer-instalogo-icon\"><use xlink:href=\"#icon-instagram-logo\"></use></svg></span>\r\n          <span class=\"insta-black-name\">{{theme_settings.instagramtitle}}</span></h2>\r\n          {{/if}}\r\n      </div>\r\n          <div class=\"home-instagram-blue align-center\">\r\n              <div class=\"insta-block clearfix\">\r\n                {{#and theme_settings.instagramId theme_settings.instagramKey}}\r\n                  <div id=\"instafeed\" class=\"\"\r\n                  data-id=\"{{theme_settings.instagramId}}\"\r\n                  data-key=\"{{theme_settings.instagramKey}}\"\r\n                  data-variation=\"{{theme_settings.instagramSectionStyle2}}\"\r\n                  data-loading=\"{{cdn 'img/loading.svg'}}\">\r\n                  </div>\r\n                {{/and}}\r\n            </div>\r\n          </div>\r\n    </article>\r\n{{/and}}\r\n"}