{"components/carousel-content":"<div data-animation-in=\"fadeIn\" class=\"animated fadeIn heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\r\n    <h2 class=\"heroCarousel-title animated slideInDown\" data-animation-in=\"slideInDown\">{{{heading}}}</h2>\r\n    <p class=\"heroCarousel-description animated slideInDown\" data-animation-in=\"slideInDown\">{{{text}}}</p>\r\n    {{#if button_text}}\r\n        <span data-animation-in=\"slideInDown\" class=\"heroCarousel-action button btn-big animated slideInDown\">{{button_text}}</span>\r\n    {{/if}}\r\n</div>\r\n"}